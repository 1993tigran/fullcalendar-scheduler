{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2015",
      "scripthost"
    ],
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": true,
    "noUnusedLocals": true,
    "noImplicitUseStrict": true,
    "importHelpers": true,
    "baseUrl": ".",
    "paths": {
      "fullcalendar": [ "fullcalendar/src/core/main" ],
      "fullcalendar-agenda": [ "fullcalendar/src/agenda/main" ]
    },
    "outDir": "tmp/tsc-output"
  },
  "include": [
    "src/main.ts"
  ]
}
